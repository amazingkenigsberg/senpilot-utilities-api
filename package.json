{
  "name": "senpilot-utilities-api",
  "version": "1.0.0",
  "description": "Standalone utilities API for CSR testing and VAPI integration",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx prisma generate && tsc",
    "start": "npx prisma migrate deploy && npx tsx src/seed.ts && node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "migrate:deploy": "npx prisma migrate deploy",
    "seed": "npx tsx src/seed.ts"
  },
  "keywords": [
    "utilities",
    "csr",
    "vapi",
    "mock-api"
  ],
  "author": "Senpilot",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "pg": "^8.16.3",
    "prisma": "^6.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  }
}
